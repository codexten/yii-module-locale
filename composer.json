{
  "name": "codexten/yii-module-locale",
  "type": "library",
  "description": "Locale module for yii 2.0 or higher",
  "require": {
    "codexten/yii-web": "^2.0",
    "symfony/intl": "*",
    "kartik-v/yii2-widget-select2": "@dev"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\locale\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "moduleLocale": "config/moduleLocale.php",
      "migrationNamespaces": "config/migrationNamespaces.php"
    }
  }
}
